
<!DOCTYPE html>
<html>

<h1> Properties </h1>
  <ul id="properties">
      <% @properties.each do |property| %>
        <li>
          <a href="<%="/properties/book:#{property.id}"%>">
            <%= property.description %>
            <%= property.price %>
            <%= User.get(property.user_id).email %>
          </a>
        </li>
    <% end %>
  </ul>

<form action="/properties/new" method="get" >
  <input type="hidden" >
   <input type="submit" value="New listing">
 </form>

